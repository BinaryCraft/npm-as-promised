{
  "name": "npm-as-promised",
  "version": "0.0.3",
  "description": "Turn the npm callback api into a promise api",
  "main": "dist/index.js",
  "scripts": {
    "compile": "babel source --out-dir dist",
    "prepublish": "npm run compile",
    "checkStyle": "jscs .",
    "lint": "jshint .",
    "qualityAssurance": "npm run checkStyle && npm run lint",
    "test": "npm run qualityAssurance && jasmine JASMINE_CONFIG_PATH=jasmine.json"
  },
  "dependencies": {
    "babel-cli": "^6.6.5",
    "jasmine-es6": "^0.1.7",
    "jscs": "^2.11.0",
    "jshint": "^2.9.1",
    "npm": "^3.8.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marthinus-engelbrecht/npm-as-promised.git"
  },
  "keywords": [
    "npm",
    "promise"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marthinus-engelbrecht/npm-as-promised/issues"
  },
  "homepage": "https://github.com/marthinus-engelbrecht/npm-as-promised#readme"
}
